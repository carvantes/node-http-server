{
  "version": 1,
  "framework": { "name": "node", "version": "18.17.0" },
  "routes": [
    {
      "path": "/*",
      "target": {
        "kind": "Compute",
        "src": "default"
      }
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "runtime": "nodejs18.x",
      "entrypoint": "server.js"
    }
  ]
}
